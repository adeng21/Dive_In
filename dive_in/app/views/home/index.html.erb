<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

<div id="main-header">
  <h1 class="home-header"> Where Do You Want To Dive? </h1>
  <%= render 'maps/map'%>
<div>

<div>
  <h1 class="home-header">Find Divesites By Category</h1><br>
  <ul class="small-block-grid-5">
    <% @categories.each do |category|%>
    <li class="text-center"><%= link_to category.name, category_path(category)%></li>
    <%end%>
  </ul>
</div>

<div>
  <h1 class="home-header">Find Divesites By Month</h1><br>
  <ul class="small-block-grid-6">
    <% @months.each do |month|%>
    <li class="text-center"><%= link_to month.name, month_path(month)%></li>
    <%end%>
  </ul>
</div>

<div>
  <h1 class="home-header"><%=link_to 'Complete List of the Best Divesites in the World', divesites_path%></h1>
</div>



